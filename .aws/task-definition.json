{
  "family": "plattii-app",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::796973489775:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "plattii-app",
      "image": "796973489775.dkr.ecr.ap-southeast-2.amazonaws.com/plattii-dev-ecr:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
